{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ],
    "import/extensions": [
      "error",
      {
        "js": "always"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "__filename",
          "__dirname"
        ]
      }
    ]
  }
}